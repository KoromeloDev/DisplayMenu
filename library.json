{
    "name": "DisplayMenu",
    "version": "1.2.3",
    "description": "DisplayMenu is a library based on the GyverOLED library for easily creating menus for SSD1306 128x64 displays",
    "keywords": "menu, diplay, ssd1306",
    "repository": {
        "type": "git",
        "url": "https://github.com/KoromeloDev/DisplayMenu.git"
    },
    "authors": [
        {
            "name": "KoromeloDev",
            "maintainer": true
        }
    ],
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "gyverlibs/GyverOLED": "~1.6.4"
    },
    "frameworks": "arduino",
    "platforms": "*",
    "build": {
        "unflags": [
            "-std=gnu++98",
            "-std=gnu++11",
            "-std=gnu++14"
        ],
        "flags": [
            "-std=gnu++17"
        ]
    }
}
